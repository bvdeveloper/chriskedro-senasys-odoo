                        </group>
                    </group>
                    <group name="studio_group_huniX">
                        <group name="studio_group_huniX_left" string="Technical Product Information"
                               attrs="{&quot;invisible&quot;: [[&quot;yesno&quot;,&quot;!=&quot;,&quot;Yes&quot;]]}">
